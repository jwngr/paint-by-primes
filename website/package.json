{
  "name": "paint-by-primes",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "firebase": "^7.13.2",
    "get-pixels": "^3.3.2",
    "history": "^4.7.2",
    "html2canvas": "^1.0.0-alpha.12",
    "lodash": "^4.17.11",
    "polished": "^3.5.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-color": "^2.17.0",
    "react-compare-image": "^2.0.4",
    "react-dom": "^16.7.0",
    "react-helmet": "^6.0.0",
    "react-loadable": "^5.5.0",
    "react-media": "^1.9.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1",
    "typeface-overpass-mono": "0.0.71",
    "typeface-roboto-mono": "0.0.75",
    "typeface-rubik": "0.0.72",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*'",
    "start": "react-scripts start",
    "build:dev": "firebase setup:web --json -P dev | jq '.result' > ./src/firebaseConfig.json && react-scripts build",
    "build:prod": "firebase setup:web --json -P prod | jq '.result' > ./src/firebaseConfig.json && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,schedules}/**/*.{js,json,css}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "redux-logger": "^3.0.6",
    "source-map-explorer": "^2.4.2"
  }
}
